{
  "name": "@ali4heydari/portfolio",
  "version": "3.0.0-alpha.01",
  "description": "My personal portfolio",
  "keywords": [
    "Nextjs",
    "typescript",
    "tailwindcss"
  ],
  "bugs": {
    "url": "https://github.com/ali4heydari/ali4heydari.github.io/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ali4heydari/ali4heydari.github.io"
  },
  "license": "MIT",
  "types": "module",
  "scripts": {
    "build": "contentlayer build && next build",
    "export": "pnpm build && next export -o build",
    "start": "next start",
    "dev": "contentlayer build && next dev",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "postinstall": "husky install",
    "lint:css": "stylelint \"**/*.{js,jsx,ts,tsx}\"",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "contentlayer:build": "contentlayer build"
  },
  "resolutions": {
    "webpack": "^5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
