{
  "name": "@ali4heydari/portfolio",
  "version": "3.0.0-alpha.01",
  "description": "My personal portfolio",
  "keywords": [
    "Nextjs",
    "typescript",
    "tailwindcss"
  ],
  "bugs": {
    "url": "https://github.com/ali4heydari/ali4heydari.github.io/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ali4heydari/ali4heydari.github.io"
  },
  "license": "MIT",
  "scripts": {
    "app:dev": "concurrently --kill-others \"pnpm velite:dev\" \"pnpm next:dev\"",
    "app:build": "pnpm velite:build && pnpm next:build",
    "velite:dev": "velite --watch --verbose --debug",
    "velite:build": "velite build",
    "next:build": "next build",
    "next:dev": "next dev",
    "next:export": "pnpm build && next export -o build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "prepare": "husky",
    "lint:css": "stylelint \"**/*.{js,jsx,ts,tsx}\"",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@headlessui/react": "^2.0.3",
    "@jsdevtools/rehype-toc": "^3.0.2",
    "@next/third-parties": "^14.2.3",
    "@sentry/nextjs": "^8.12.0",
    "@tailwindcss/typography": "^0.5.2",
    "@tanstack/react-query": "^5.28.4",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "autoprefixer": "^10.4.18",
    "axios": "^1.6.8",
    "cheerio": "1.0.0-rc.12",
    "classnames": "^2.5.1",
    "disqus-react": "^1.1.2",
    "mdi-react": "^8.6.0",
    "million": "^2.6.4",
    "next": "^14.2.3",
    "next-themes": "^0.3.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-calendly": "^4.3.0",
    "react-dom": "^18.2.0",
    "react-hotjar": "^6.3.1",
    "react-web-share": "^2.0.2",
    "reading-time": "^1.5.0",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-code-titles": "^1.0.3",
    "rehype-pretty-code": "^0.13.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "4.0.0",
    "sharp": "^0.33.2",
    "shikiji": "0.10.2",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-hocus": "^0.0.7",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.2",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "@ngneat/falso": "^7.2.0",
    "@storybook/addon-actions": "^8.0.0",
    "@storybook/addon-backgrounds": "^8.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/nextjs": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/gtag.js": "^0.0.19",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
