    "test:e2e": "start-server-and-test 'yarn build && yarn serve' http://localhost:9000 cy:open",
    "test:e2e:ci": "start-server-and-test 'yarn build && yarn serve' http://localhost:9000 cy:run",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
